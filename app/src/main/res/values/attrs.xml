<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagsEditText">
        <attr name="allowSpaceInTag" format="boolean"/>
        <attr name="tagsTextColor" format="color"/>
        <attr name="tagsTextSize" format="dimension"/>
        <attr name="tagsBackground" format="integer"/>
        <attr name="tagsCloseImageLeft" format="integer"/>
        <attr name="tagsCloseImageRight" format="integer"/>
        <attr name="tagsCloseImagePadding" format="dimension" />
        <attr name="tagsPaddingLeft" format="dimension"/>
        <attr name="tagsPaddingRight" format="dimension"/>
        <attr name="tagsPaddingTop" format="dimension"/>
        <attr name="tagsPaddingBottom" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="StepIndicator">
        <attr name="siStepCount" format="integer" />
        <attr name="siRadius" format="dimension" />
        <attr name="siStrokeWidth" format="dimension" />
        <attr name="siStepColor" format="color" />
        <attr name="siCurrentStepColor" format="color" />
        <attr name="siBackgroundColor" format="color" />
        <attr name="siTextColor" format="color" />
        <attr name="siSecondaryTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LatoTextView">
        <attr name="typeface"/>
        <attr name="fontFamily"/>
        <attr name="textWeight"/>
        <attr name="textStyle"/>
    </declare-styleable>

    <declare-styleable name="LatoButton">
        <attr name="typeface"/>
        <attr name="fontFamily"/>
        <attr name="textWeight"/>
        <attr name="textStyle"/>
    </declare-styleable>

    <declare-styleable name="LatoEditText">
        <attr name="typeface"/>
        <attr name="fontFamily"/>
        <attr name="textWeight"/>
        <attr name="textStyle"/>
    </declare-styleable>

    <declare-styleable name="LatoRadioButton">
        <attr name="typeface"/>
        <attr name="fontFamily"/>
        <attr name="textWeight"/>
        <attr name="textStyle"/>
    </declare-styleable>

    <declare-styleable name="LatoCheckBox">
        <attr name="typeface"/>
        <attr name="fontFamily"/>
        <attr name="textWeight"/>
        <attr name="textStyle"/>
    </declare-styleable>

    <attr name="typeface">
        <enum name="lato_regular" value="4"/>
        <enum name="lato_bold" value="8"/>

    </attr>

    <attr name="fontFamily">
        <enum name="lato" value="0"/>
    </attr>

    <attr name="textWeight">
        <enum name="normal" value="0"/>
        <enum name="bold" value="4"/>
    </attr>

    <attr name="textStyle">
        <enum name="normal" value="0"/>
    </attr>
</resources>